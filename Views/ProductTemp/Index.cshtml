@model List<Product>

@{
    ViewData["Title"] = "List Products (Temp)";
}

<div id="Filter" class="row mt-5">
        <partial name="_SortFilterPartial" model=" Model"/>
</div>


<div id="cards" class="row mt-3">
    @foreach (var item in Model)
    {
        <partial name="_ProductCardPartial" model="item" />
    }
</div>